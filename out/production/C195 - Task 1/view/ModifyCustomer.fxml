<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ModifyCustomer">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Modify Customer">
            <font>
                <Font name="System Bold" size="24.0" />
            </font>
        </Label>
        <GridPane layoutX="100.0" layoutY="85.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Label text="Customer ID" />
                <Label text="Customer Name" GridPane.rowIndex="1" />
                <Label text="Phone" GridPane.rowIndex="2" />
                <Label text="Address" GridPane.rowIndex="3" />
                <Label text="Postal Code" GridPane.rowIndex="4" />
                <Label text="Country" GridPane.rowIndex="5" />
                <Label text="Division" GridPane.rowIndex="6" />
                <TextField fx:id="modCustId" GridPane.columnIndex="1" />
                <TextField fx:id="modCustName" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <TextField fx:id="modCustNum" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <TextField fx:id="modCustAddress" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                <TextField fx:id="modCustPost" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <ComboBox fx:id="modCustCountry" onAction="#onActionCountry" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <ComboBox fx:id="modCustDiv" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="6" />
            </children>
        </GridPane>
        <HBox layoutX="200.0" layoutY="415.0" prefHeight="15.0" prefWidth="97.0" spacing="5.0">
            <children>
                <Button mnemonicParsing="false" onAction="#onActionSave" text="Save" />
                <Button mnemonicParsing="false" onAction="#onActionCancel" text="Cancel" />
            </children>
        </HBox>
    </children>
</AnchorPane>
